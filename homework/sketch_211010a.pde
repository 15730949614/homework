float x;
float y;
float a;
float b;
float r;
float g;
float p;
void setup(){
  size(640,640);
  x = 460;
  y = 200;
  a = 100;
  b = 100;
}

void draw(){
  background(225);
  stroke(0);
  line(200,0,200,640);
  line(0,400,640,400);
  line(300,0,300,640);
  line(550,0,550,400);
  line(0,460,200,460);
  line(0,580,200,580);
  line(50,460,50,640);
  line(150,400,150,580);
  line(0,y,200,y);
  y = y + 1;
  if(y>380){
    y = 200;
  }
  fill(225);
  quad(500,400,640,400,640,640,500,640);
  fill(250,250,250);
  quad(300,400,500,400,500,x,300,x);
  rect(550,300,90,100);
  line(400,x,400,640);
  x = x+0.5;
  if(x>600){
    x =460;
  }
  fill(180);
  rect(0,0,200,200);
  rect(300,200,250,200);
  
  fill(210);
  rect(200,0,a,b);
  line(200,b,550,b);
  a = a+0.3;
  b = b+0.5;
  line((200+200+a)/2,0,(200+200+a)/2,b);
  line(200,b/2,(200+200+a)/2,b/2);
  if(a>225){
    a = 100;
  }
  if(b>225){
    b = 100;
  }
  fill(180);
  rect(50,460,100,120);
  rect(500,520,140,120);
  rect(150,400,50,60);
}
